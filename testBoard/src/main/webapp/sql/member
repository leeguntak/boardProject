CREATE TABLE boardMembers(
	mem_id varchar2(50) PRIMARY KEY,
	mem_pwd char(60),
	mem_name varchar2(50),
    mem_tel1 varchar2(20),
    mem_tel2 varchar2(20),
    mem_tel3 varchar2(20),
    mem_email1 varchar2(70),
    mem_email2 varchar2(70),
	mem_postcode varchar2(100),
    mem_address varchar2(100),
	mem_detailAddress varchar2(100),
    mem_extraAddress varchar2(100),
	mem_logtime DATE DEFAULT SYSDATE NOT NULL
);